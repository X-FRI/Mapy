    "@mdit/plugin-tab": "^0.5.2",
    "asciidoctor": "^3.0.0",
    "assist-worker": "^1.1.0",
    "blob-stream": "^0.1.3",
    "codemirror": "^6.0.1",
    "d3": "^7.8.5",
    "d3-cloud": "^1.2.7",
