{
  "name": "mapy",
  "version": "0.1.0",
  "description": "A Markdown Editor and Viewer.",
  "main": "app/main.js",
  "scripts": {
    "start": "clear && electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.tanpero.mapy",
    "productName": "Mapy",
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "logo.ico",
      "uninstallerIcon": "logo.ico",
      "installerHeaderIcon": "logo.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Mapy"
    }
  },
  "win": {
    "target": "nsis",
    "icon": "logo.ico"
  },
  "dependencies": {
    "@arothuis/markdown-it-biblatex": "^1.0.1",
    "@codemirror/lang-markdown": "^6.2.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@luckrya/markdown-it-link-to-card": "^0.3.3",
    "@mdit/plugin-tab": "^0.5.2",
    "asciidoctor": "^3.0.0",
    "assist-worker": "^1.1.0",
    "blob-stream": "^0.1.3",
    "codemirror": "^6.0.1",
    "d3": "^7.8.5",
    "file-saver": "^2.0.5",
    "fontmin": "^1.0.1",
    "highlight.js": "^11.9.0",
    "highlightjs-cypher": "^1.1.5",
    "html2canvas": "^1.4.1",
    "iconify-icon": "^1.0.8",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.7.0",
    "katex": "^0.16.9",
    "mammoth": "^1.6.0",
    "markdown-it": "^13.0.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-adobe-plugin": "^1.1.7",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-bidi": "^0.1.0",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-codetabs": "^1.2.0",
    "markdown-it-collapsible": "^2.0.0",
    "markdown-it-color": "^2.1.1",
    "markdown-it-colorful-checkbox": "^1.0.2",
    "markdown-it-complex-table": "^1.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-copy": "^1.2.0",
    "markdown-it-easy-tables": "^0.1.1",
    "markdown-it-emoji-mart": "^0.1.1",
    "markdown-it-fancy-lists": "^1.2.3",
    "markdown-it-fontawesome": "^0.3.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-highlightjs": "^4.0.1",
    "markdown-it-html5-media": "^0.7.1",
    "markdown-it-image-caption": "^1.0.0",
    "markdown-it-implicit-figures": "^0.12.0",
    "markdown-it-implicit-figures-video": "^0.10.1",
    "markdown-it-inject-linenumbers": "^0.3.0",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-kbd": "^2.2.2",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-marked": "^0.3.3",
    "markdown-it-modify-token": "^1.0.2",
    "markdown-it-multicolumn": "^0.1.5",
    "markdown-it-multimd-table": "^4.2.3",
    "markdown-it-named-code-blocks": "^0.2.0",
    "markdown-it-regexp": "^0.4.0",
    "markdown-it-relativelink": "^0.4.2",
    "markdown-it-replace-it": "^1.0.0",
    "markdown-it-replace-link": "^1.2.0",
